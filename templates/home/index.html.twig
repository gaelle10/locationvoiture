{% extends 'base.html.twig' %}
{% block title %}Home{% endblock %}

{% block body %}
    <main>
        <!-- banner-area -->
        <section class="banner-area fix" data-background="img/slider/banner_bg.jpg">
            <div class="container custom-container">
                <div class="row align-items-center justify-content-between">
                    <div class="col-lg-6 order-0 order-lg-2">
                        <div class="banner-img">
                            <img src="img/slider/banner_img.png" alt="">
                            <img class="banner-shape" src="img/slider/banner_shape.png" alt="">
                        </div>
                    </div>
                    <div class="col-xl-5 col-lg-6">
                        <div class="slider-content">
                            <span class="wow fadeInUp" data-wow-delay=".3s">MEILLEUR VENDEUR DE VOITURES</span>
                            <h2 class="wow fadeInUp" data-wow-delay=".6s">Concessionnaire de véhicules populaires</h2>
                            <p class="wow fadeInUp" data-wow-delay=".9s">Trouvez la voiture idéale avec notre outil de recherche de voiture</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="banner-overlay-text">Location</div>
            <div class="banner-line-wrap">
                <span class="line-one"></span>
                <span class="line-two"></span>
            </div>
        </section>


        {% for vehicule in vehicules %}
            <div class="inventory-list-item">
                <div class="inventory-list-thumb">
                    <img src="{{ vehicule.photo }}" alt="">
                </div>
                <div class="inventory-list-content">
                    <div class="inv-content-top">
                        <ul>
                            <li class="price">Eur {{ vehicule.prixJournalier }}<span>/jour</span></li>
                        </ul>
                    </div>
                    <h6>{{ vehicule.marque }}</h6>
                    <h4>{{ vehicule.titre }}</h4>
                    <p class="location">
                        {{ vehicule.description }}
                    </p>
                    <a href="{{ path('app_commander', {id: vehicule.id}) }}" class="btn btn-primary">Commander</a>
                </div>
            </div>
        {% endfor %}

